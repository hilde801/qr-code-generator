{
	"name": "qr-code-generator",
	"displayName": "QR Code Generator",
	"version": "prototype-6",
	"description": "A simple web application to generate QR codes.",
	"type": "module",
	"author": "Hilde801",
	"license": "Apache-2.0",
	"scripts": {
		"dev": "npx vite",
		"build": "npx tsc -b && npx vite build",
